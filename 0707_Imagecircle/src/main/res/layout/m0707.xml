<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    tools:context=".M0707">

    <TextView
        android:id="@+id/m0707_t001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="88dp"
        android:layout_marginBottom="161dp"
        android:text="@string/m0707_t001"
        android:textColor="@color/Black"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="62dp"
        android:text="@string/myname"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize" />

    <tw.tcnr08.m0707.CircleImgView
        android:id="@+id/Imageview01"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_above="@+id/m0707_t001"
        android:layout_alignStart="@+id/m0707_t001"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="125dp"
        android:layout_marginBottom="95dp"
        android:background="#00000000"
        android:scaleType="matrix"
        app:srcCompat="@drawable/img15th" />

    <tw.tcnr08.m0707.CircleImgbutton
        android:id="@+id/circleImgbutton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignStart="@+id/m0707_t001"
        android:layout_alignBottom="@+id/m0707_t001"
        android:layout_centerVertical="true"
        android:layout_marginStart="110dp"
        android:layout_marginBottom="64dp"
        android:background="#00000000"
        app:srcCompat="@drawable/o9898"
        tools:ignore="SpeakableTextPresentCheck" />

</RelativeLayout>